  providers: [DatafetcherComponent],
   
  constructor(
    private comp: DatafetcherComponent
  ) { }

let high:any = await this.comp.getData('high')
let low: any = await this.comp.getData('low')

let that = this
let inputValue: any = document.getElementById('chart')
inputValue.onclick = function (evt: any) {
  that.chart.data.labels = that.chart.data.labels.length + 1
  that.chart.data.datasets[0].data.push(400)
  that.chart.update()
}
inputValue.onclick = function (evt: any) {
  var activePoints = that.chart.getElementsAtEventForMode(evt, 'point', that.chart.options)
  var firstPoint = activePoints[0]
  var value = that.chart.data.datasets[firstPoint.datasetIndex]
  alert(value)
}